	<!-- Root Context: defines shared resources visible to all other web components -->
	<bean name="userdao" class="com.yue.dao.UserDAO" />
	<bean name="user" class="com.yue.pojo.User" />	
	<bean name="userorder" class="com.yue.pojo.UserOrder" />
